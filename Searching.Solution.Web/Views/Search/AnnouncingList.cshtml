@*@*@using SearchingLibrary;
@model List<Announcing>*@


    
        <div class="ibox" ng-repeat="ann in AnnList">
            <a href="/">
                <div class="objectava">
                        <img class="img-thumbnail img-responsive thumbnail-border" src="images/2.jpg"
                             width="40" height="40" alt="Nickname" />
                </div>
            </a>
                <div class="tags">
                    <a href=""><p>{{ann.UserName}} {{ann.UserLastName}}</p></a>
                </div>
                <div class="date" ng-click="AddtoSelected(ann.Announcing_id)">
                    dd:mm:yyyy

                </div>

                <div class="advertisements">
                    <div class="nameadvertisements">
                        <!-- Announce title --> 
                        {{ann.Name_Announcing}}
                    </div>

                    <!-- Announce text --> 
                    {{ann.Info_Announcing}}



                </div>

</div>*@

